<Window x:Class="SL_App.EmailWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SL_App"
        mc:Ignorable="d"
        Title="EmailWindow" Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <DockPanel DockPanel.Dock="Bottom" Height="30" LastChildFill="True" Margin="2 0">
            <Button DockPanel.Dock="Right" Width="60" Content="Send" Margin="2 0"/>
            <TextBox/>
        </DockPanel>
        <WebBrowser x:Name="webBrowser">
            
        </WebBrowser>
    </DockPanel>
</Window>
